---
const { url, label, target } = Astro.props;
---

<a
  href={url}
  target={target}
  class="text-sm font-bold px-6 py-4 leading-none border rounded text-white bg-brand-green hover:bg-brand-green-dark mt-4 lg:mt-0"
  data-goatcounter-click="altagenda.crossuite.com/MOOV/"
  data-goatcounter-title="Appointment">{label}</a
>
