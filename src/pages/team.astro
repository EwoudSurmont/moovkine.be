---
import Layout from "../layouts/Layout.astro";
import { Image } from "astro:assets";
import mainCourseData from "../data/mainCourseData";
import TeamCard from "../components/TeamCard.astro";

const { title = "Team" } = Astro.props;
---

<Layout title={title}>
  <div class="mx-auto px-8 md:px-16 py-20 md:py-40 text-center">
    <h1 class="font-sans text-4xl font-bold pb-20 md:pb-40">{title}</h1>
    <div class="max-w-screen-lg mx-auto flex flex-col gap-12">
      <TeamCard firstName="Sanne" />
      <TeamCard firstName="Jara" />
    </div>
  </div>
</Layout>
